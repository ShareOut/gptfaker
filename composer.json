{
  "name": "shareout/gptfaker",
  "description": "An package that integrates with FakerPHP and generates AI based texts",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Motivo\\GptFaker\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Jonathan Hafkamp",
      "email": "jonathan@motivo.nl"
    },
    {
      "name": "Nicolas Karlen",
      "email": "nicolas.karlen@gmail.com"
    }
  ],
  "require": {
    "php": "^8",
    "fakerphp/faker": "^1.23",
    "tectalic/openai": "^1.5",
    "illuminate/support": "^9|^10"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motivo\\GptFaker\\GptFakerServiceProvider"
      ]
    }
  }
}
